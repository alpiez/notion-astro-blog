---
import type { Heading2BlockObjectResponse } from "@notionhq/client/build/src/api-endpoints";

interface Props {
  content: Heading2BlockObjectResponse;
}

let { content } = Astro.props;
let head2 = null;

if (content.heading_2.rich_text[0]?.type === "text") {
  head2 = content.heading_2.rich_text[0].text.content;
}
---

<div class="text-3xl font-bold mt-16 mb-8">
  {head2}
</div>
